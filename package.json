{
  "name": "tracker_v2",
  "version": "1.0.0",
  "description": "Express and react app for tracking student words",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
