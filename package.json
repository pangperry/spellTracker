    "jq": "^1.7.2",
    "jsonwebtoken": "^8.2.1",
    "lodash.isempty": "^4.4.0",
    "mongoose": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.4"
  }
